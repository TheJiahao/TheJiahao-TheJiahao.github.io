{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "headers": [
        {
            "source": "/",
            "has": [
                {
                    "type": "host",
                    "value": "[\\s\\S]*"
                }
            ],
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "noindex"
                }
            ]
        }
    ]
}
