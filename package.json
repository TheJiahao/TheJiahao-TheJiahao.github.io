{
    "name": "jiahao-blog",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "format": "markdownlint --fix . && zhlint *.md --fix && prettier --write . && taplo format",
        "measure": "hugo --templateMetrics --templateMetricsHints",
        "measure:nocache": "hugo --templateMetrics --templateMetricsHints --ignoreCache",
        "index": "gis $SITE_URL"
    },
    "devDependencies": {
        "@taplo/cli": "^0.7.0",
        "google-indexing-script": "^0.0.5",
        "markdownlint-cli": "^0.40.0",
        "prettier": "^3.2.5",
        "prettier-plugin-go-template": "^0.0.15",
        "zhlint": "^0.8.1"
    }
}
